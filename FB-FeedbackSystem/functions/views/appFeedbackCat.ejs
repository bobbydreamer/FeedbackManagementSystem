<!DOCTYPE html>
<html lang="en">

<!-- https://mdbootstrap.com/previews/templates/admin-dashboard/html/dashboards/v-1.html -->

<head>
    <% include partials/template/head.ejs %>

    <!-- Your custom styles (optional) -->
    <style>
        select.browser-default {
            display: inline!important;
            font-size: .74rem;
            background-color: transparent;
            color: #2bbbad;
            border: 2px solid #2bbbad!important;
            padding: .38rem;
            margin: .375rem;
        }
    
        option:disabled, select[disabled] > option {
            color: #6c757d;
        }
    
        option{
            color:#000;
        }
    
        .date-align {
            text-align: right!important;
        }

        @media only screen and (max-width: 800px) {
            .date-align { text-align: left!important; }
        }    
    </style>
</head>

<body id="<%= pageID %>" class="fixed-sn white-skin" data-category="<%= categoryName %>" style="display:none">

    <!--Main Navigation-->
    <header>
        <% include partials/template/navs.ejs %>
    </header>
    <!--Main Navigation-->

    <!--Main layout-->
    <main>
        <div class="container-fluid">

            <!--Section: Title 2-->
            <section class="mt-1">
                <!--Grid row-->
                <div class="row">
                    <h1 class="ml-1" id="application"></h1> 
                </div>
                <!--Grid row-->                    
            </section>

            <!--Section: Latest feedback cards -->
            <section class="mt-lg-2" id="latestFeedbacks">

            </section>            

            <div style="height: 5px"></div>

            <!--Section: Main panel-->
            <section class="mb-5" id="latest200Messages-placeholder">
            </section>
            <!--Section: Main panel-->            

            </div>
    </main>
    <!--Main layout-->

    <!--Footer-->
    <% include partials/template/footer.ejs %>
    <script defer type="text/javascript" src="../js/fbs-common.js"></script>
    <script defer type="text/javascript" src="../js/appFeedbackCat.js"></script>

</body>

</html>