    <!--Initializations-->
    <script>
        // SideNav Initialization
        $(".button-collapse").sideNav();

        var container = document.querySelector('.custom-scrollbar');
        Ps.initialize(container, {
            wheelSpeed: 2,
            wheelPropagation: true,
            minScrollbarLength: 20
        });

        // Data Picker Initialization
        $('.datepicker').pickadate();

        // Material Select Initialization
        $(document).ready(function () {
            $('.mdb-select').material_select();
        });

        // Tooltips Initialization
        $(function () {
            $('[data-toggle="tooltip"]').tooltip()
        })
    </script>

<!-- Handlebar -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/4.0.11/handlebars.js"></script>

<!-- Handlebar.js template-->
<script id="sidebar-template" type="text/x-handlebars-template">
  {{#appNames}}
    <li><a href="/app/{{shortName}}" class="collapsible-header waves-effect"><i class="fa fa-th-large"></i> {{longName}}</a></li>
  {{/appNames}}
</script>

<script id="ACallApps-template" type="text/x-handlebars-template">
    {{#appNames}}
    <tr>
        <th>{{shortName}}</th>
        <td>{{dateSaved}}</td>
        <td>{{description}}</td>
        <td>{{longName}}</td>
        <td>
            <a class="teal-text" data-toggle="tooltip" data-placement="top" title="Edit"><i class="fa fa-pencil"></i></a>
            &nbsp;
            <a class="red-text" data-toggle="tooltip" data-placement="top" title="Remove"><i class="fa fa-times"></i></a>            
        </td>
    </tr>
    {{/appNames}}
</script>
  
  <script id="userCategories-template" type="text/x-handlebars-template">
    {{#catNames}}
    <tr>
        <td><p style="font-size:20px; margin:0px; padding:0px;"><a href = "/category/{{catName}}" target="_blank"><span class="badge red">{{catName}}</span></a></p></td>
        <td>{{counts}}</td>
        <td><button class="delete" id="{{catName}}"><i class="fa fa-times"></i></button></td>
        
    </tr>    
    {{/catNames}}
  </script>
  
  <script id="appList-template" type="text/x-handlebars-template">
    {{#appNames}}
    <option id="{{id}}" value="{{appName}}">{{appName}}</option>
    {{/appNames}}
  </script>
  
  <script id="appCounts-template" type="text/x-handlebars-template">
    {{#appCounts}}

        <!--Grid column-->
        <div class="col-xl-3 col-md-6 mb-4">
            <a href="/app/{{shortName}}" target="_blank">
            <!--Card Unread Counts-->
            <div class="card classic-admin-card {{color}}">
                <div class="card-body">
                    <div class="pull-right">
                        <i class="fa fa-comments"></i>
                    </div>
                    <p class="white-text">{{longName}}</p>
                    <h4>{{WeeklyCount}}/{{TotalCount}}</h4>
                </div>
            </div>
            <!--/.Card Unread Counts-->
            </a>
        </div>
        <!--Grid column-->

    {{/appCounts}}
  </script>
  
  <script id="latest200Messages-template" type="text/x-handlebars-template">
    {{#latest200Messages}}
    <section class="mb-4">
        <div class="card">
            <div class="card-body unique-color-dark white-text">

                <!--Grid row-->
                <div class="row">

                    <!--Grid column-->
                    <div class="col-md-9 text-left">
                        <p class="blue-text mb-1"><strong>{{subject}}</strong></p>
                        <p><small>
                            <span class="amber-text">{{application}}</span>
                            <span class="blue-text">&nbsp;/&nbsp;</span>
                            <span class="yellow-text">{{name}}</span>
                            <span class="blue-text">&nbsp;/&nbsp;</span>
                            {{email}}
                        </small></p>
                    </div>
                    <!--Grid column-->

                    <!--Grid column-->
                    <div class="col-md-3 date-align orange-text">

                        <small>{{dateSaved}}</small>

                    </div>
                    <!--Grid column-->
                    
                    <div class="card-body pt-1 pb-1" id="divMessage">{{message}}</div>

                </div>
                <!--Grid row-->

                <div class="row mt-3">
                    <div class="col-md-6">
                        <!--Blue select-->
                        <select class="browser-default" data-application={{shortName}} data-curCategory={{category}}>                            
                            {{#each CategoryList}}
                                {{{this}}}
                            {{/each}}                                                                    
                        </select>
                        <!--/Blue select-->                                            
                        <button type="button" id="setCategory" class="btn btn-outline-default waves-effect btn-sm" data-id="{{mid}}">set category</button>
                    </div>
                    
                    <div class="col-md-6 date-align">
                        <button type="button" id="btnArchive" class="btn btn-outline-default waves-effect btn-sm" data-id="{{mid}}" data-application={{shortName}} {{{dis}}}>archive and forget</button>
                    </div>                                                                    
                </div>

            </div>
        </div>    
    </section>                

    {{/latest200Messages}}
  </script>

<script id="allUsers-template" type="text/x-handlebars-template">
    {{#userList}}
    <tr>
        <td>{{username}}</td>
        <td>{{email}}</td>
        <td>{{uid}}</td>
        <td>{{lastLogin}}</td>
        <td><button class="delete" id="{{uid}}"><i class="fa fa-times"></i></button></td>
    </tr>
    {{/userList}}
</script>
  